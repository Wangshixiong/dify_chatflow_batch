# Chatflow 自动化测试工具配置文件模板
# 使用说明：
# 1. 复制此文件为 config.ini
# 2. 填入真实的API信息
# 3. 确保config.ini不会被提交到版本控制系统

[API]
# Dify API的基础URL，通常格式为：http://your-domain/v1
API_URL = http://localhost/v1

# Dify应用的API密钥，从Dify控制台获取
API_KEY = your_api_key_here

# 用户ID，用于标识API调用者
USER_ID = test_user

# API调用超时时间（秒）
TIMEOUT = 30

[LOGGING]
# 日志级别：DEBUG, INFO, WARNING, ERROR
LOG_LEVEL = INFO

# 日志文件路径（可选，留空则只输出到控制台）
LOG_FILE = 

[OUTPUT]
# 结果文件输出目录（可选，留空则输出到当前目录）
OUTPUT_DIR = 

# 是否在文件名中包含时间戳
INCLUDE_TIMESTAMP = true